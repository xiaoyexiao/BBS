<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.library.mapper.ManagerMapper">
    <select id="getManagerByID" resultType="Manager">
        select * from manager
        where number=#{number};
    </select>
</mapper>